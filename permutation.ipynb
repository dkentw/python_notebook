    "        print(s)\n",
    "    \n",
    "    for i in range(l, r):\n",
    "        s[l], s[i] = s[i], s[l]\n",
    "        result = permutate(s, l+1, r)\n",
    "        s[l], s[i] = s[i], s[l]\n",
    "        \n",
    "    \n",
    "s = '123'\n",
    "permutate(list(s), 0, len(s))"
